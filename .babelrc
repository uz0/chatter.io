{
  "env": {
    "test": {
      "plugins": ["@babel/plugin-transform-runtime"]
    }
  },

  "presets": [
    "@babel/preset-env",
    "@babel/preset-react"
  ],

  "plugins": [
    "reselector/babel",
    "@babel/plugin-proposal-class-properties",

    ["module-resolver", {
      "alias": {
        "@": "./src"
      }
    }]
  ]
}